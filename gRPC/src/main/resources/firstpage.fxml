<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sd.Assignment3.controller.JavaFXController">
   <children>
      <TableView fx:id="tableView" layoutX="98.0" layoutY="76.0" prefHeight="448.0" prefWidth="804.0">
        <columns>
          <TableColumn fx:id="patient" prefWidth="75.0" text="Patient" />
          <TableColumn fx:id="medication" prefWidth="75.0" text="Medication" />
            <TableColumn fx:id="startDate" prefWidth="75.0" text="Start Date" />
            <TableColumn fx:id="endDate" prefWidth="75.0" text="End Date" />
            <TableColumn fx:id="dosage" prefWidth="75.0" text="Dosage" />
            <TableColumn fx:id="intakeTime" prefWidth="75.0" text="Intake Time" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="takeMedication" layoutX="450.0" layoutY="548.0" mnemonicParsing="false" onAction="#takeMedication" text="Take Medication" />
      <Label fx:id="time" layoutX="37.0" layoutY="26.0" text="Time: 00:00" />
   </children>
</AnchorPane>
